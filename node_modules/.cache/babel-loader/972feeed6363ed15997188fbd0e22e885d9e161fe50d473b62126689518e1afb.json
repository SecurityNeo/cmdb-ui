{"ast":null,"code":"import { reactive, toRefs, onMounted, getCurrentInstance, ref } from 'vue';\nimport qs from 'qs';\nexport default {\n  setup() {\n    const {\n      proxy\n    } = getCurrentInstance();\n    const config = reactive({\n      centerDialogVisible: true,\n      isLoading: false\n    });\n    const selectUserId = ref(NaN);\n    const server = reactive({\n      hdm_ip: \"\",\n      hdm_user_name: \"\",\n      hdm_password: \"\",\n      vendor: \"\",\n      product_name: \"\",\n      arch: \"\",\n      sn: \"\",\n      mgt_ip: \"\",\n      mgt_root_pwd: \"\",\n      storage_ip: \"\",\n      owner_id: NaN,\n      host_name: \"\",\n      tag_id: NaN,\n      location: \"\",\n      description: \"\",\n      hardware_cpu: NaN,\n      hardware_mem: NaN,\n      hardware_storage: \"\"\n    });\n    const user = reactive({\n      user_arr: []\n    });\n    onMounted(() => {\n      getUserList();\n    });\n\n    // 查询用户列表\n    const getUserList = async () => {\n      const query = qs.stringify({\n        page: 1,\n        size: 1000\n      });\n      try {\n        const rest = await new proxy.$request(proxy.$urls.m().userlist + \"?\" + query).modeget();\n        if (rest.data.code !== 200) {\n          new proxy.$tips(\"获取用户列表失败\", \"warning\").notify();\n        }\n        user.user_arr = rest.data.data;\n      } catch (e) {\n        new proxy.$tips(\"服务未就绪，请稍后重试\", \"error\").notify();\n      }\n    };\n    const selectOwner = userId => {\n      server.owner_id = userId;\n      console.log(server.owner_id);\n    };\n    return {\n      ...toRefs(server),\n      config,\n      user,\n      selectOwner,\n      selectUserId\n    };\n  }\n};","map":{"version":3,"names":["reactive","toRefs","onMounted","getCurrentInstance","ref","qs","setup","proxy","config","centerDialogVisible","isLoading","selectUserId","NaN","server","hdm_ip","hdm_user_name","hdm_password","vendor","product_name","arch","sn","mgt_ip","mgt_root_pwd","storage_ip","owner_id","host_name","tag_id","