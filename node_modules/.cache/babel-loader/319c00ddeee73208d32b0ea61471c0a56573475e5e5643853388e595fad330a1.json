{"ast":null,"code":"import { getCurrentInstance, onMounted, reactive, toRefs, ref } from 'vue';\nimport addTagDialog from './components/addtag-dialog.vue';\nimport updateTagDialog from './components/updatetag-dialog.vue';\nimport deleteTagDialog from './components/deletetag-dialog.vue';\nimport qs from 'qs';\nexport default {\n  components: {\n    addTagDialog,\n    updateTagDialog,\n    deleteTagDialog\n  },\n  methods: {\n    formatTime(row, column, cellValue) {\n      return new Date(cellValue).toLocaleString();\n    }\n  },\n  setup() {\n    const {\n      proxy\n    } = getCurrentInstance();\n    const addTagDialog = ref();\n    const updateTagDialog = ref();\n    const deleteTagDialog = ref();\n    const tags = reactive({\n      tag_arr: [],\n      page_size: 10,\n      page: 1,\n      total: 0\n    });\n    onMounted(() => {\n      getTagList();\n    });\n\n    // 查询资源标签列表\n    const getTagList = async () => {\n      const query = qs.stringify({\n        page: tags.page,\n        size: tags.page_size\n      });\n      try {\n        const rest = await new proxy.$request(proxy.$urls.m().tagList + \"?\" + query).modeget();\n        if (rest.data.code !== 200) {\n          new proxy.$tips(\"获取资源标签列表失败\", \"warning\").notify();\n        }\n        tags.tag_arr = rest.data.data;\n      } catch (e) {\n        if (e.code !== 401) {\n          new proxy.$tips(\"服务器错误，请稍后重试\", \"error\").notify();\n        }\n      }\n    };\n\n    // 更改每页展示条数后触发\n    function handleSizeChange(val) {\n      users.page_size = val;\n      getTagList();\n    }\n\n    // 更改页码后触发\n    function handleCurrentChange(val) {\n      users.page = val;\n      getTagList();\n    }\n    function clickAddTag() {\n      addTagDialog.value.addTagDialog();\n    }\n    function clickUpdateTag(tagDetail) {\n      updateTagDialog.value.updateTagDialog(tagDetail);\n    }\n    function clickDelete(tagId) {\n      deleteTagDialog.value.deleteTagDialog(tagId);\n    }\n    return {\n      ...toRefs(tags),\n      handleSizeChange,\n      handleCurrentChange,\n      clickAddTag,\n      clickUpdateTag,\n      clickDelete,\n      getTagList,\n      addTagDialog,\n      updateTagDialog,\n      deleteTagDialog\n    };\n  }\n};","map":{"version":3,"names":["getCurrentInstance","onMounted","reactive","toRefs","ref","addTagDialog","updateTagDialog","deleteTagDialog","qs","components","methods","formatTime","row","column","cellValue","Date","toLocaleString","setup","proxy","tags","tag_arr","page_size","page","total","getTagList","query","stringify","size","rest","$request","$urls","m","tagList","modeget","data","code","$tips","notify","e","handleSizeChange","val","users","handleCurrentChange","clickAddTag","value","clickUpdateTag","tagDetail","clickDelete","tagId"],"sources":["D:\\src\\github.com\\SecurityNeo\\cmdb-ui\\src\\page\\resource\\tag\\tag.vue"],"sourcesContent":["<template>\r\n    <div class=\"oderding\">\r\n        <div class=\"heading\">\r\n            <div>资源标签管理</div>\r\n\r\n            <!-- 表格上方功能按钮 -->\r\n            <div class=\"table-head\">\r\n                <el-button type=\"primary\" @click=\"clickAddTag\" >新建标签</el-button>\r\n            </div>\r\n            \r\n            <div>\r\n                <!-- 用户表格 -->\r\n                <div>          \r\n                    <el-table :data=\"tag_arr\" stripe   style=\"width: 100% \" class=\"cell\">\r\n                        <el-table-column prop=\"name\" label=\"名称\" width= \"240px\"/>\r\n                        <el-table-column prop=\"description\" label=\"描述\" />\r\n                        <el-table-column prop=\"CreatedAt\" :formatter=\"formatTime\" label=\"创建时间\" width=\"140px\" />\r\n                        <el-table-column prop=\"UpdatedAt\" :formatter=\"formatTime\" label=\"更新时间\"  width=\"140px\" />\r\n                        <el-table-column fixed=\"right\" label=\"操作\" width=\"120\">\r\n                            <template #default=\"scope\">\r\n                                <el-button link type=\"primary\" size=\"small\" v-if=\"scope.row.id === 1\" disabled>编辑</el-button>\r\n                                <el-button link type=\"primary\" size=\"small\" v-else @click=\"clickUpdateTag(scope.row)\" >编辑</el-button>\r\n                                <el-button link type=\"primary\" size=\"small\" v-if=\"scope.row.id === 1\" disabled>删除</el-button>\r\n                                <el-button link type=\"primary\" size=\"small\" v-else @click=\"clickDelete(scope.row.id)\" >删除</el-button>\r\n                            </template>\r\n                        </el-table-column>   \r\n                    </el-table>\r\n                </div>\r\n\r\n                <!-- 分页模块 -->\r\n                <div class=\"pagination\">\r\n                    <el-pagination\r\n                    v-model:current-page=\"page\"\r\n                    v-model:page-size=\"page_size\"\r\n                    :page-sizes=\"[10, 20, 50, 100]\"\r\n                    small\r\n                    background\r\n                    layout=\"total, sizes, prev, pager, next, jumper\"\r\n                    :total=\"total\"\r\n                    @size-change=\"handleSizeChange\"\r\n                    @current-change=\"handleCurrentChange\"\r\n                    />\r\n                </div>\r\n            </div>\r\n        </div>\r\n        <addTagDialog ref=\"addTagDialog\" @flushPage=\"getTagList\"> </addTagDialog>\r\n        <updateTagDialog ref=\"updateTagDialog\" @flushPage=\"getTagList\"></updateTagDialog>\r\n        <deleteTagDialog  ref=\"deleteTagDialog\" @flushPage=\"getTagList\" ></deleteTagDialog>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n\r\nimport { getCurrentInstance,onMounted, reactive,toRefs,ref } from 'vue'\r\nimport addTagDialog from './components/addtag-dialog.vue'\r\nimport updateTagDialog from './components/updatetag-dialog.vue'\r\nimport deleteTagDialog from './components/deletetag-dialog.vue'\r\nimport qs from 'qs'\r\n\r\nexport default {\r\n    components: {\r\n        addTagDialog,\r\n        updateTagDialog,\r\n        deleteTagDialog\r\n    },\r\n    methods: {\r\n        formatTime(row, column,cellValue){\r\n            return new Date(cellValue).toLocaleString()\r\n        }\r\n    },\r\n    setup() {\r\n        const {proxy} = getCurrentInstance()\r\n\r\n        const addTagDialog = ref()\r\n        const updateTagDialog = ref()\r\n        const deleteTagDialog = ref()\r\n\r\n        const tags = reactive(\r\n            {\r\n                tag_arr: [],\r\n                page_size: 10,\r\n                page: 1,\r\n                total: 0\r\n            }                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                         