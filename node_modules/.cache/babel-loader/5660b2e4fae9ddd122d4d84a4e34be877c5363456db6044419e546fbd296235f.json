{"ast":null,"code":"import { getCurrentInstance, onMounted, reactive, toRefs, ref } from 'vue';\nimport qs from 'qs';\nexport default {\n  setup() {\n    const {\n      proxy\n    } = getCurrentInstance();\n    const tags = reactive({\n      tag_arr: [],\n      page_size: 10,\n      page: 1,\n      total: 0\n    });\n    onMounted(() => {\n      getTagList();\n    });\n\n    // 查询资源标签列表\n    const getTagList = async () => {\n      const query = qs.stringify({\n        page: 1,\n        size: 1000\n      });\n      try {\n        const rest = await new proxy.$request(proxy.$urls.m().tagList + \"?\" + query).modeget();\n        if (rest.data.code !== 200) {\n          new proxy.$tips(\"获取资源标签列表失败\", \"warning\").notify();\n        }\n        tag.tag_arr = rest.data.data;\n      } catch (e) {\n        if (e.code !== 401) {\n          new proxy.$tips(\"服务器错误，请稍后重试\", \"error\").notify();\n        }\n      }\n    };\n\n    // 更改每页展示条数后触发\n    function handleSizeChange(val) {\n      users.page_size = val;\n      getTagList();\n    }\n\n    // 更改页码后触发\n    function handleCurrentChange(val) {\n      users.page = val;\n      getTagList();\n    }\n    return {\n      ...toRefs(tags),\n      handleSizeChange,\n      handleCurrentChange\n    };\n  }\n};","map":{"version":3,"names":["getCurrentInstance","onMounted","reactive","toRefs","ref","qs","setup","proxy","tags","tag_arr","page_size","page","total","getTagList","query","stringify","size","rest","$request","$urls","m","tagList","modeget","data","code","$tips","notify","tag","e","handleSizeChange","val","users","handleCurrentChange"],"sources":["D:\\src\\github.com\\SecurityNeo\\cmdb-ui\\src\\page\\resource\\tag\\tag.vue"],"sourcesContent":["<template>\r\n    <div class=\"oderding\">\r\n        <div class=\"heading\">\r\n            <div>资源标签管理</div>\r\n\r\n            <!-- 表格上方功能按钮 -->\r\n            <div class=\"table-head\">\r\n                <el-button type=\"primary\" @click=\"clickAdduser\" >标签用户</el-button>\r\n            </div>\r\n            \r\n            <div>\r\n                <!-- 用户表格 -->\r\n                <div>          \r\n                    <el-table :data=\"user_arr\" stripe   style=\"width: 100% \" class=\"cell\">\r\n                        <el-table-column prop=\"phone\" label=\"名称\" />\r\n                        <el-table-column prop=\"mail\" label=\"描述\" />\r\n                        <el-table-column prop=\"CreatedAt\" label=\"创建时间\"  />\r\n                        <el-table-column fixed=\"right\" label=\"操作\" width=\"120\">\r\n                            <template #default=\"scope\">\r\n                                <!-- 系统管理员不允许编辑与删除 -->\r\n                                <el-button link type=\"primary\" size=\"small\"  @click=\"clickUpdateTags(scope.row)\" >编辑</el-button>\r\n                                <el-button link type=\"primary\" size=\"small\"  @click=\"clickDelete(scope.row.id)\" >删除</el-button>\r\n                            </template>\r\n                        </el-table-column>   \r\n                    </el-table>\r\n                </div>\r\n\r\n                <!-- 分页模块 -->\r\n                <div class=\"pagination\">\r\n                    <el-pagination\r\n                    v-model:current-page=\"page\"\r\n                    v-model:page-size=\"page_size\"\r\n                    :page-sizes=\"[10, 20, 50, 100]\"\r\n                    small\r\n                    background\r\n                    layout=\"total, sizes, prev, pager, next, jumper\"\r\n                    :total=\"total\"\r\n                    @size-change=\"handleSizeChange\"\r\n                    @current-change=\"handleCurrentChange\"\r\n                    />\r\n                </div>\r\n            </div>\r\n        </div>\r\n\r\n\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n\r\nimport { getCurrentInstance,onMounted, reactive,toRefs, ref  } from 'vue'\r\nimport qs from 'qs'\r\n\r\nexport default {\r\n    setup() {\r\n        const {proxy} = getCurrentInstance()\r\n        const tags = reactive(\r\n            {\r\n                tag_arr: [],\r\n                page_size: 10,\r\n                page: 1,\r\n                total: 0\r\n            }                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                      \r\n        )\r\n\r\n        onMounted(()=>{\r\n            getTagList()\r\n        })\r\n\r\n        // 查询资源标签列表\r\n        const getTagList = async()=> {\r\n            const query = qs.stringify({\r\n                page: 1,\r\n                size: 1000,\r\n            })\r\n            try {\r\n                const rest = await new proxy.$request(proxy.$urls.m().tagList + \"?\" + query).modeget()\r\n                \r\n                if (rest.data.code !== 200 ) {\r\n                    new proxy.$tips(\"获取资源标签列表失败\", \"warning\").notify()\r\n                }\r\n                tag.tag_arr = rest.data.data\r\n                \r\n            } catch (e) {\r\n                if (e.code !== 401) {\r\n                    new proxy.$tips(\"服务器错误，请稍后重试\", \"error\").notify()\r\n                }   \r\n            }\r\n        }\r\n\r\n        // 更改每页展示条数后触发\r\n        function handleSizeChange(val) {\r\n            users.page_size = val\r\n            getTagList()\r\n        }\r\n\r\n        // 更改页码后触发\r\n        function handleCurrentChange(val) {\r\n            users.page = val\r\n            getTagList()\r\n        }\r\n\r\n\r\n        return {\r\n            ...toRefs(tags), \r\n            handleSizeChange, \r\n            handleCurrentChange\r\n        }\r\n    }\r\n}\r\n</script>\r\n\r\n<style>\r\n\r\n</style>"],"mappings":"AAkDA,SAASA,kBAAkB,EAACC,SAAS,EAAEC,QAAQ,EAACC,MAAM,EAAEC,GAAE,QAAU,KAAI;AACxE,OAAOC,EAAC,MAAO,IAAG;AAElB,eAAe;EACXC,KAAKA,CAAA,EAAG;IACJ,MAAM;MAACC;IAAK,IAAIP,kBAAkB,CAAC;IACnC,MAAMQ,IAAG,GAAIN,QAAQ,CACjB;MACIO,OAAO,EAAE,EAAE;MACXC,SAAS,EAAE,EAAE;MACbC,IAAI,EAAE,CAAC;MACPC,KAAK,EAAE;IACX,CACJ;IAEAX,SAAS,CAAC,MAAI;MACVY,UAAU,CAAC;IACf,CAAC;;IAED;IACA,MAAMA,UAAS,GAAI,MAAAA,CAAA,KAAU;MACzB,MAAMC,KAAI,GAAIT,EAAE,CAACU,SAAS,CAAC;QACvBJ,IAAI,EAAE,CAAC;QACPK,IAAI,EAAE;MACV,CAAC;MACD,IAAI;QACA,MAAMC,IAAG,GAAI,MAAM,IAAIV,KAAK,CAACW,QAAQ,CAACX,KAAK,CAACY,KAAK,CAACC,CAAC,CAAC,CAAC,CAACC,OAAM,GAAI,GAAE,GAAIP,KAAK,CAAC,CAACQ,OAAO,CAAC;QAErF,IAAIL,IAAI,CAACM,IAAI,CAACC,IAAG,KAAM,GAAE,EAAI;UACzB,IAAIjB,KAAK,CAACkB,KAAK,CAAC,YAAY,EAAE,SAAS,CAAC,CAACC,MAA