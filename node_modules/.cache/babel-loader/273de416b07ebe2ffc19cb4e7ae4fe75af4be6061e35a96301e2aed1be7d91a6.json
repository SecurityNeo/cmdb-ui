{"ast":null,"code":"import { reactive, getCurrentInstance } from 'vue';\nexport default {\n  setup() {\n    const {\n      proxy\n    } = getCurrentInstance();\n    const res = reactive({\n      centerDialogVisible: false,\n      tagId: 0,\n      isLoading: false\n    });\n\n    //删除服务器数据\n    const deleteTag = async () => {\n      res.isLoading = true;\n      const rest = await new proxy.$request(proxy.$urls.m().tagDelete + \"?id=\" + res.tagId).modedelete();\n      if (rest.data.code !== 200) {\n        new proxy.$tips(\"删除标签数据失败，稍后重试\", \"error\").notify();\n      }\n      res.centerDialogVisible = false;\n      proxy.$emit('flushPage');\n      res.isLoading = false;\n    };\n\n    // 被父组件调用\n    function deleteTagDialog(tagId) {\n      res.centerDialogVisible = true;\n      res.tagId = tagId;\n    }\n    return {\n      res,\n      deleteTag,\n      deleteTagDialog\n    };\n  }\n};","map":{"version":3,"names":["reactive","getCurrentInstance","setup","proxy","res","centerDialogVisible","tagId","isLoading","deleteTag","rest","$request","$urls","m","tagDelete","modedelete","data","code","$tips","notify","$emit","deleteTagDialog"],"sources":["D:\\src\\github.com\\SecurityNeo\\cmdb-ui\\src\\page\\resource\\tag\\components\\deletetag-dialog.vue"],"sourcesContent":["<template>\r\n    <div>\r\n        <el-dialog\r\n            v-model=\"res.centerDialogVisible\"\r\n            title=\"提示\"\r\n            width=\"30%\"\r\n            align-center\r\n            :close-on-click-modal=\"false\"\r\n            \r\n        >\r\n            <span>数据删除后不可恢复，确认要删除本条数据吗？</span>\r\n            <template #footer>\r\n            <span >\r\n                <el-button @click=\"res.centerDialogVisible = false\">取消</el-button>\r\n                <el-button type=\"primary\" @click=\"deleteTag\" :loading=\"res.isLoading\">确认</el-button>\r\n            </span>\r\n            </template>\r\n        </el-dialog>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport {reactive, getCurrentInstance} from 'vue'\r\n\r\n\r\nexport default {\r\n    setup() {\r\n        const {proxy} = getCurrentInstance()\r\n        const res = reactive({\r\n            centerDialogVisible: false,\r\n            tagId: 0,\r\n            isLoading: false \r\n        })\r\n        \r\n        //删除服务器数据\r\n        const deleteTag = async() => {\r\n            res.isLoading = true\r\n           \r\n            const rest = await new proxy.$request(proxy.$urls.m().tagDelete + \"?id=\" + res.tagId).modedelete()\r\n      \r\n            if (rest.data.code !== 200 ) {\r\n                new proxy.$tips(\"删除标签数据失败，稍后重试\", \"error\").notify()\r\n            } \r\n            res.centerDialogVisible = false\r\n            proxy.$emit('flushPage')\r\n            res.isLoading = false        \r\n           \r\n        }\r\n\r\n        // 被父组件调用\r\n        function deleteTagDialog(tagId) {\r\n            res.centerDialogVisible = true\r\n            res.tagId = tagId\r\n        }\r\n\r\n        return {res, deleteTag,deleteTagDialog}\r\n    }\r\n}\r\n</script>"],"mappings":"AAsBA,SAAQA,QAAQ,EAAEC,kBAAkB,QAAO,KAAI;AAG/C,eAAe;EACXC,KAAKA,CAAA,EAAG;IACJ,MAAM;MAACC;IAAK,IAAIF,kBAAkB,CAAC;IACnC,MAAMG,GAAE,GAAIJ,QAAQ,CAAC;MACjBK,mBAAmB,EAAE,KAAK;MAC1BC,KAAK,EAAE,CAAC;MACRC,SAAS,EAAE;IACf,CAAC;;IAED;IACA,MAAMC,SAAQ,GAAI,MAAAA,CAAA,KAAW;MACzBJ,GAAG,CAACG,SAAQ,GAAI,IAAG;MAEnB,MAAME,IAAG,GAAI,MAAM,IAAIN,KAAK,CAACO,QAAQ,CAACP,KAAK,CAACQ,KAAK,CAACC,CAAC,CAAC,CAAC,CAACC,SAAQ,GAAI,MAAK,GAAIT,GAAG,CAACE,KAAK,CAAC,CAACQ,UAAU,CAAC;MAEjG,IAAIL,IAAI,CAACM,IAAI,CAACC,IAAG,KAAM,GAAE,EAAI;QACzB,IAAIb,KAAK,CAACc,KAAK,CAAC,eAAe,EAAE,OAAO,CAAC,CAACC,MAAM,CAAC;MACrD;MACAd,GAAG,CAACC,mBAAkB,GAAI,KAAI;MAC9BF,KAAK,CAACgB,KAAK,CAAC,WAAW;MACvBf,GAAG,CAACG,SAAQ,GAAI,KAAI;IAExB;;IAEA;IACA,SAASa,eAAeA,CAACd,KAAK,EAAE;MAC5BF,GAAG,CAACC,mBAAkB,GAAI,IAAG;MAC7BD,GAAG,CAACE,KAAI,GAAIA,KAAI;IACpB;IAEA,OAAO;MAACF,GAAG;MAAEI,SAAS;MAACY;IAAe;EAC1C;AACJ"},"metadata":{},"sourceType":"module","externalDependencies":[]}