{"ast":null,"code":"import { reactive, toRefs, getCurrentInstance } from 'vue';\nimport { Base64 } from 'js-base64';\nexport default {\n  setup() {\n    const user = reactive({\n      account: \"\",\n      name_alias: \"\",\n      password: \"\",\n      re_password: \"\",\n      mail: \"\",\n      phone: \"\"\n    });\n    const rules = {\n      account: [{\n        required: true,\n        message: '请填写内容',\n        trigger: 'blur'\n      }, {\n        pattern: /^[a-zA-Z0-9_]+$/,\n        message: '只能输入字母、数字和下划线',\n        trigger: 'blur'\n      }]\n    };\n    const config = reactive({\n      centerDialogVisible: false,\n      isLoading: false\n    });\n    const {\n      proxy\n    } = getCurrentInstance();\n    const addUser = async () => {\n      const obj = {\n        username: user.account,\n        name_alias: user.name_alias,\n        password: Base64.encode(user.password),\n        mail: user.mail,\n        phone: user.phone,\n        role: \"user\"\n      };\n      config.isLoading = true;\n      const rest = await new proxy.$request(proxy.$urls.m().useradd, obj).modepost();\n      if (rest.data.code !== 200) {\n        new proxy.$tips(\"创建用户失败，稍后重试\", \"error\").notify();\n      }\n      config.isLoading = false;\n      config.centerDialogVisible = false;\n      proxy.$emit('flushPage');\n    };\n\n    // 被父组件user.vue调用\n    function addUserDialog() {\n      config.centerDialogVisible = true;\n    }\n    return {\n      ...toRefs(user),\n      rules,\n      config,\n      addUser,\n      addUserDialog\n    };\n  }\n};","map":{"version":3,"names":["reactive","toRefs","getCurrentInstance","Base64","setup","user","account","name_alias","password","re_password","mail","phone","rules","required","message","trigger","pattern","config","centerDialogVisible","isLoading","proxy","addUser","obj","username","encode","role","rest","$request","$urls","m","useradd","modepost","data","code","$tips","notify","$emit","addUserDialog"],"sources":["D:\\src\\github.com\\Secu