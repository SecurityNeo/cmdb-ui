n                <div>\r\n                    <div>公共属性</div>\r\n                    <div class=\"input-float \">\r\n                        <div class=\"input_key\">\r\n                            备注信息\r\n                        </div>\r\n                        <div class=\"input_value\">\r\n                            <el-input v-model=\"description\" clearable class=\"inputflex\"  placeholder=\"请输服务器备注信息\"  />\r\n                    </div>       \r\n                </div>\r\n\r\n                <!-- 表格下方功能按钮 -->\r\n                <div style=\"float: right; margin-bottom: 40px; margin-right: 72px;margin-top: 20px;\">\r\n                    <el-button @click=\"config.centerDialogVisible = false\">取消</el-button>\r\n                    <el-button type=\"primary\" :loading=\"config.isLoading\" >确认</el-button>\r\n                </div>\r\n            </div>\r\n            </div>\r\n        </el-dialog>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport { reactive } from 'vue'\r\n\r\nexport default ({\r\n    setup() {\r\n        const config = reactive({\r\n            centerDialogVisible: true,\r\n            isLoading: false\r\n        })\r\n\r\n\r\n        return {\r\n            config\r\n        }\r\n    }\r\n    \r\n})\r\n</script>\r\n"],"mappings":"AAkMA,SAASA,QAAO,QAAS,KAAI;AAE7B,eAAgB;EACZC,KAAKA,CAAA,EAAG;IACJ,MAAMC,MAAK,GAAIF,QAAQ,CAAC;MACpBG,mBAAmB,EAAE,IAAI;MACzBC,SAAS,EAAE;IACf,CAAC;IAGD,OAAO;MACHF;IACJ;EACJ;AAEJ,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}